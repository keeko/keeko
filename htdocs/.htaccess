RewriteEngine On
RewriteBase /keeko/htdocs

RewriteRule ^modules/(.*)$ ../keeko/modules/$1 [L]
RewriteRule ^libs/(.*)$ ../libs/$1 [L]

RewriteCond %{REQUEST_FILENAME} -s [OR]
RewriteCond %{REQUEST_FILENAME} -l [OR]
RewriteCond %{REQUEST_FILENAME} -d
RewriteRule ^.*$ - [NC,L]
RewriteRule ^(.*)$ index.php?k=$1 [NC,L]